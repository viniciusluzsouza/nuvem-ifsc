# Loading ieee.std_logic_1164(body)
# Loading ieee.numeric_std(body)
# Loading work.mod10_counter(two_seg_arch)
add wave -position end  sim:/mod10_counter/clk
add wave -position end  sim:/mod10_counter/reset
add wave -position end  sim:/mod10_counter/n
add wave -position end  sim:/mod10_counter/q
add wave -position end  sim:/mod10_counter/r_reg
add wave -position end  sim:/mod10_counter/r_next
# ** Error: Unable to lock WLF file "vsim.wlf". Retrying 1 times, errno 11
# ** Error: Unable to lock WLF file "vsim.wlf". Retrying 2 times, errno 11
